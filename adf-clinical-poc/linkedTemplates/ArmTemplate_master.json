{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-clinical-poc"
		},
		"LS_AzureDatabricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_AzureDatabricks'"
		},
		"ls_adls_clinical_poc_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_clinical_poc'"
		},
		"ls_mysql_local_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_mysql_local'"
		},
		"LS_AzureDatabricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1012-172738-ocalcask"
		},
		"ls_ADLS_DATASOURCE_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsclinicalpoc.dfs.core.windows.net/"
		},
		"ls_adls_clinical_poc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsclinicalpoc.dfs.core.windows.net/"
		},
		"ls_mysql_local_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "localhost"
		},
		"ls_mysql_local_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "clinical_db"
		},
		"ls_mysql_local_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "root"
		},
		"ls_rest_clinical_api_poc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://clinical-api-poc.azurewebsites.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-clinical-poc_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_clinical_poc_accountKey": {
						"value": "[parameters('ls_adls_clinical_poc_accountKey')]"
					},
					"LS_AzureDatabricks_accessToken": {
						"value": "[parameters('LS_AzureDatabricks_accessToken')]"
					},
					"ls_mysql_local_password": {
						"value": "[parameters('ls_mysql_local_password')]"
					},
					"ls_ADLS_DATASOURCE_properties_typeProperties_url": {
						"value": "[parameters('ls_ADLS_DATASOURCE_properties_typeProperties_url')]"
					},
					"ls_adls_clinical_poc_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_clinical_poc_properties_typeProperties_url')]"
					},
					"ls_rest_clinical_api_poc_properties_typeProperties_url": {
						"value": "[parameters('ls_rest_clinical_api_poc_properties_typeProperties_url')]"
					},
					"LS_AzureDatabricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('LS_AzureDatabricks_properties_typeProperties_existingClusterId')]"
					},
					"ls_mysql_local_properties_typeProperties_server": {
						"value": "[parameters('ls_mysql_local_properties_typeProperties_server')]"
					},
					"ls_mysql_local_properties_typeProperties_database": {
						"value": "[parameters('ls_mysql_local_properties_typeProperties_database')]"
					},
					"ls_mysql_local_properties_typeProperties_username": {
						"value": "[parameters('ls_mysql_local_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}